{
  "name": "WebRTC Protect",
  "short_name": "iwprotect",
  "description": "Prevent WebRTC from leaking your private and public IPs",
  "author": "Joe Ertaba",
  "version": "0.1.2",
  "manifest_version": 2,
  "permissions": [
    "storage",
    "privacy",
    "contextMenus"
  ],
  "background": {
    "persistent": false,
    "scripts": [
      "common.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "16": "data/icons/16.png",
      "18": "data/icons/18.png",
      "19": "data/icons/19.png",
      "32": "data/icons/32.png",
      "48": "data/icons/48.png",
      "64": "data/icons/64.png"
    }
  },
  "homepage_url": "http://add0n.com/webrtc-protect.html",
  "icons": {
    "16": "data/icons/16.png",
    "18": "data/icons/18.png",
    "19": "data/icons/19.png",
    "32": "data/icons/32.png",
    "48": "data/icons/48.png",
    "64": "data/icons/64.png",
    "128": "data/icons/128.png"
  },
  "content_scripts": [{
    "matches": ["*://*/*"],
    "js": ["data/inject.js"]
  }],
  "options_ui": {
    "page": "data/options/index.html",
    "chrome_style": true
  },
  "applications": {
    "gecko": {
      "id": "{4bfcf5ab-fb35-4da0-bce3-d8ad07adb08f}",
      "strict_min_version": "55.0"
    }
  }
}
