{
  "name": "WebRTC Protect",
  "short_name": "iwprotect",
  "description": "Prevent WebRTC from leaking your private and public IPs",
  "author": "Joe Ertaba",
  "version": "0.1.0",
  "manifest_version": 2,
  "permissions": [
    "storage",
    "privacy",
    "contextMenus"
  ],
  "background": {
    "persistent": false,
    "scripts": [
      "common.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "16": "data/icons/16.png",
      "32": "data/icons/32.png"
    }
  },
  "homepage_url": "http://add0n.com/webrtc-protect.html",
  "icons": {
    "16": "data/icons/16.png",
    "48": "data/icons/48.png",
    "128": "data/icons/128.png"
  },
  "options_ui": {
    "page": "data/options/index.html",
    "chrome_style": true
  }
}
